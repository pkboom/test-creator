{
    "name": "pkboom/test-creator",
    "description": "Creates tests for a package",
    "keywords": [
        "pkboom",
        "test-creator"
    ],
    "homepage": "https://github.com/pkboom/test-creator",
    "license": "MIT",
    "authors": [
        {
            "name": "Keunbae Park",
            "email": "ppotpo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/collections": "^8.76|^9.0",
        "illuminate/support": "^8.76|^9.0",
        "symfony/console": "^5.0|^6.0",
        "symfony/finder": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/process": "^6.0",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pkboom\\TestCreator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "test-creator"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}